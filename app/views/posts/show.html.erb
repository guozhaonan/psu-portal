<p id="notice"><%= notice %></p>

<p>
  <strong>Title:</strong>
  <%= @post.title %>
</p>

<p>
  <strong>Tagline:</strong>
  <%= @post.tagline %>
</p>

<p>
  <strong>Url:</strong>
  <%= @post.url %>
</p>

<p>
  <strong>Description:</strong>
  <%= to_html(@post.description).html_safe %>
</p>

<p>
  <strong>Embed Link:</strong>
  <%= @post.embed_field %>
</p>

<%= link_to 'Edit', edit_post_path(@post) %> |
